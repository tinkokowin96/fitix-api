{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/fitix-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/fitix-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/fitix-api",
  "projects": {
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "permission": {
      "type": "application",
      "root": "apps/permission",
      "entryFile": "main",
      "sourceRoot": "apps/permission/src",
      "compilerOptions": {
        "tsConfigPath": "apps/permission/tsconfig.app.json"
      }
    },
    "community": {
      "type": "application",
      "root": "apps/community",
      "entryFile": "main",
      "sourceRoot": "apps/community/src",
      "compilerOptions": {
        "tsConfigPath": "apps/community/tsconfig.app.json"
      }
    },
    "class": {
      "type": "application",
      "root": "apps/class",
      "entryFile": "main",
      "sourceRoot": "apps/class/src",
      "compilerOptions": {
        "tsConfigPath": "apps/class/tsconfig.app.json"
      }
    },
    "payment": {
      "type": "application",
      "root": "apps/payment",
      "entryFile": "main",
      "sourceRoot": "apps/payment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment/tsconfig.app.json"
      }
    },
    "misc": {
      "type": "application",
      "root": "apps/misc",
      "entryFile": "main",
      "sourceRoot": "apps/misc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/misc/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "spotlight": {
      "type": "application",
      "root": "apps/spotlight",
      "entryFile": "main",
      "sourceRoot": "apps/spotlight/src",
      "compilerOptions": {
        "tsConfigPath": "apps/spotlight/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}
